<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.humin.tmetestapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:name=".TmeApp"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:screenOrientation="portrait"
        android:theme="@style/AppTheme">
        <activity android:name=".activitys.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".activitys.wallpapers.Activity2"
            android:theme="@style/AppTheme" />

        <activity
            android:name=".activitys.wallpapers.Activity1"
            android:theme="@style/AppTheme" />

        <activity
            android:name=".activitys.wallpapers.MainWallpaperActivity"
            android:theme="@style/AppTheme" />

        <activity
            android:name=".activitys.AddActivity"
            android:theme="@style/AppTheme" />
    </application>

</manifest>